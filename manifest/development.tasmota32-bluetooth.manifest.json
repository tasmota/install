{
  "name": "Development Tasmota32 Bluetooth (english)",
  "home_assistant_domain": "tasmota",
  "new_install_prompt_erase": true,
  "new_install_improv_wait_time": 10,
  "builds": [
    {
      "chipFamily": "ESP32",
      "improv": true,
      "parts": [
        { "path": "../firmware/development/tasmota32-bluetooth.factory.bin", "offset": 0 }
      ]
    },
    {
      "chipFamily": "ESP32-C3",
      "improv": true,
      "parts": [
        { "path": "../firmware/unofficial//tasmota32c3-bluetooth.factory.bin", "offset": 0 }
      ]
    },
    {
      "chipFamily": "ESP32-S3",
      "improv": true,
      "parts": [
        { "path": "../firmware/unofficial//tasmota32s3-bluetooth.factory.bin", "offset": 0 }
      ]
    }
  ]
}
